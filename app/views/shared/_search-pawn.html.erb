<div class='interest_form'>
  <%= form_with(url: process, method: :get, local: true) do |f| %>
  <%# form_with(scope: :search, url: process, method: :get, local: true) do |f| %>
    <div class="field">
      <div>
      質データIDを入力してください
      </div>
      <div class='short-form-field flex_left'>
        <div class='short_text_area'>
          <%= select_tag 'column' ,options_for_select([['質番号', 'id'], ['姓(漢字)', 'last_name'], ['姓(カタカナ)', 'last_name_kana']]) %>
          <%= f.text_area :word, class:"pawn-search-word", id:"pawn-search-word", size:"10x1", maxlength:"16" %>
        </div>
        <div>
          <%= f.submit "検索" ,class:"short-text-register", data: { confirm: 'この条件で検索していいですか?' } %>
        </div>
      </div>
    </div>
  <% end %>
</div>