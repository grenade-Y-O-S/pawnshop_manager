<div class='form_layout'>
  <div class='second_frame'>
    <div class="search-form">
      <%= form_with(scope: :search, url: process, method: :get, local: true) do |f| %>
        <div class="field">
          <div>
          質データIDを入力してください
          </div>
          <%= text_field_tag(:word) %>
        </div>
        <div class='register-btn'>
          <%= f.submit "検索" ,class:"register-btn", data: { confirm: 'この条件で検索していいですか?' } %>
        </div>
      <% end %>
    </div>
  </div>
</div>